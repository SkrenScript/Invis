	SGUI10.TextScaled = true
	SGUI10.TextSize = 14.000
	SGUI10.TextWrapped = true
	SGUI10.MouseButton1Down:connect(function()

		local ScreenGui = Instance.new("ScreenGui")
		local main = Instance.new("Frame")
		local label = Instance.new("TextLabel")
		local Speed = Instance.new("TextButton")
		local JP = Instance.new("TextButton")


		ScreenGui.Parent = game.CoreGui

		main.Name = "main"
		main.Parent = ScreenGui
		main.BackgroundColor3 = Color3.fromRGB(35, 39, 42)
		main.Position = UDim2.new(0.46938774, 0, 0.743083, 0)
		main.Size = UDim2.new(0, 352, 0, 158)
		main.Active = true
		main.Draggable = true

		label.Name = "label"
		label.Parent = main
		label.BackgroundColor3 = Color3.fromRGB(86, 99, 247)
		label.Size = UDim2.new(0, 352, 0, 25)
		label.Font = Enum.Font.GothamBold
		label.Text = "S GUI  |  Made by Simon"
		label.TextColor3 = Color3.fromRGB(0, 0, 0)
		label.TextSize = 26.000
		label.TextXAlignment = Enum.TextXAlignment.Left

		Speed.Name = "Speed"
		Speed.Parent = main
		Speed.BackgroundColor3 = Color3.fromRGB(86, 99, 247)
		Speed.BorderSizePixel = 2
		Speed.Position = UDim2.new(0.0340909101, 0, 0.240506336, 0)
		Speed.Size = UDim2.new(0, 106, 0, 36)
		Speed.Style = Enum.ButtonStyle.RobloxRoundDefaultButton
		Speed.Font = Enum.Font.Cartoon
		Speed.Text = "Speed (dw)"
		Speed.TextColor3 = Color3.fromRGB(0, 0, 0)
		Speed.TextScaled = true
		Speed.TextSize = 14.000
		Speed.TextWrapped = true
		Speed.MouseButton1Down:connect(function()
			local Player = game.Players.PlayerYouIndex

			-- Check if they have a Character
			if Player.Character then
				-- Get the humanoid and set the walkspeed
				Player.Character.Humanoid.WalkSpeed = 50
			end
		end)

		JP.Name = "CMD-X"
		JP.Parent = main
		JP.BackgroundColor3 = Color3.fromRGB(86, 99, 247)
		JP.BorderSizePixel = 2
		JP.Position = UDim2.new(0.0340909101, 0, 0.632911384, 0)
		JP.Size = UDim2.new(0, 106, 0, 36)
		JP.Style = Enum.ButtonStyle.RobloxRoundDefaultButton
		JP.Font = Enum.Font.Cartoon
		JP.Text = "CMD-X"
		JP.TextColor3 = Color3.fromRGB(0, 0, 0)
		JP.TextScaled = true
		JP.TextSize = 14.000
		JP.TextWrapped = true
		JP.MouseButton1Down:connect(function()
			loadstring(game:HttpGet("https://raw.githubusercontent.com/CMD-X/CMD-X/master/Source", true))()
		end)

	end)
end)

